{
    "name": "angular2-social-auth",
    "version": "3.0.2",
    "description": "Simple client side social authentication with providers for Google, Facebook and Linkedin for Angular2 application.",
    "main": "dist/bundles/angular2-social-login.min.js",
    "module": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "transpile": "ngc",
        "package": "rollup -c",
        "minify": "uglifyjs dist/bundles/angular2-social-login.js --screw-ie8 --compress --mangle --comments --output dist/bundles/angular2-social-login.min.js",
        "build": "npm run transpile && npm run package && npm run minify"
    },
    "peerDependencies": {
        "@angular/core": "2.x || 4.x",
        "rxjs": "^5.4.1"
    },
    "devDependencies": {
        "@angular/common": "^4.2.5",
        "@angular/compiler": "^4.2.5",
        "@angular/compiler-cli": "^4.2.5",
        "@angular/core": "^4.2.5",
        "@angular/http": "^4.2.5",
        "@angular/platform-browser": "^4.2.5",
        "@types/core-js": "^0.9.42",
        "rollup": "^0.43.0",
        "rxjs": "^5.4.1",
        "typescript": "^2.4.1",
        "uglify-js": "^3.0.23",
        "zone.js": "^0.8.12"
    },
    "keywords": [
        "angular2",
        "angular",
        "angular 4",
        "authentication",
        "social auth",
        "angular4 social auth",
        "angular2 social auth"
    ],
